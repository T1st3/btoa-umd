    "respond": {
      "main": "dest/respond.min.js"
    },
    "json3": {
      "main": "lib/json3.min.js"
    },
    "mocha": {
      "main": [
        "mocha.js",
        "mocha.css"
      ]
    },
    "chai": {
      "main": "chai.js"
    },
    "chai-jquery": {
      "main": "chai-jquery.js"
    },
    "jshint": {
      "main": "dist/jshint.js"
    },
    "lodash": {
      "main": "dist/lodash.min.js"
    },
    "t1st3-assets": {
      "main": [
        "**/*.js",
        "**/*.css",
        "**/*.png",
        "**/*.jpg",
        "**/*.gif",
        "**/*.ico",
        "**/*.svg"
      ]
    }
