{
  "name": "btoa-umd",
  "version": "0.7.2",
  "homepage": "http://www.tiste.org/btoa-umd",
  "description": "A UMD module for btoa()",
  "keywords": [
    "umd",
    "btoa",
    "base64",
    "ascii",
    "binary"
  ],
  "license": "MIT",
  "authors": [
    "T1st3 <contact@tiste.org> (http://www.tiste.org/)"
  ],
  "main": [
    "./dist/btoa-umd.js",
    "./dist/btoa-umd.min.js",
    "./dist/btoa-umd.js.map"
  ],
  "ignore": [
    ".directory",
    ".DS_Store",
    ".git",
    ".gitignore",
    ".gitattributes",
    ".mailmap",
    "node_modules",
    "package.json",
    ".npmignore",
    "bower_components",
    ".bowerrc",
    ".editorconfig",
    ".jshintrc",
    ".jscs.json",
    ".istanbul.yml",
    ".travis.yml",
    "src",
    "test",
    "gh-pages",
    "docs",
    "gulpfile.js",
    "README.md"
  ],
  "dependencies": {
    "requirejs": "~2.1"
  },
  "devDependencies": {
    "t1st3-assets": "~1.6"
  },
  "overrides": {
    "requirejs": {
      "main": "require.js"
    },
    "jquery": {
      "main": [
        "dist/jquery.min.js",
        "dist/jquery.min.map"
      ]
    },
    "bootstrap": {
      "main": [
        "dist/js/bootstrap.min.js",
        "dist/css/bootstrap.min.css",
        "dist/fonts/*"
      ]
    },
    "font-awesome": {
      "main": [
